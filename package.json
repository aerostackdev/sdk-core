{
  "name": "@aerostack/sdk",
  "version": "0.6.6",
  "description": "The official Aerostack SDK for server-side Workers and client authentication.",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./gateway/billing": {
      "types": "./dist/gateway/billing.d.ts",
      "import": "./dist/gateway/billing.mjs",
      "require": "./dist/gateway/billing.js"
    },
    "./gateway/primitives": {
      "types": "./dist/gateway/primitives.d.ts",
      "import": "./dist/gateway/primitives.mjs",
      "require": "./dist/gateway/primitives.js"
    },
    "./community": {
      "types": "./dist/community.d.ts",
      "import": "./dist/community.mjs",
      "require": "./dist/community.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/gateway/billing.ts src/gateway/primitives.ts src/community.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/gateway/billing.ts src/gateway/primitives.ts --format cjs,esm --watch --dts",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "keywords": [
    "aerostack",
    "cloudflare",
    "workers",
    "auth",
    "sdk",
    "backend",
    "d1",
    "postgres",
    "edge"
  ],
  "author": "Aerostack Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aerostackdev/sdks.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/aerostackdev/sdks/issues"
  },
  "homepage": "https://docs.aerostack.dev",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aerostack/core": "^0.6.6",
    "@neondatabase/serverless": "^0.9.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
